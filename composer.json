{
    "name": "dicr/yii2-admin",
    "type": "yii2-extension",
    "license": "Apache-2.0",
    "description": "Admin module widgets for Yii2",
    "homepage": "https://gitlab.com/dicr/yii2-admin",
    "authors": [
        {
            "name": "Igor Tarasov",
            "email": "develop@dicr.org",
            "homepage": "http://dicr.org",
            "role": "developer"
        }
    ],
    "keywords": ["dicr", "php", "yii2", "admin"],
    "support": {
        "source": "https://gitlab.com/dicr/yii2-admin.git"
    },
    "autoload": {
        "psr-4": {
            "dicr\\admin\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "yiisoft/yii2": "~2.0.35",
        "yiisoft/yii2-bootstrap4": "~2.0.8",
        "dicr/yii2-asset": "~1.5.3",
        "dicr/yii2-cache": "~2.0.7",
        "dicr/yii2-file": "~4.0.10",
        "dicr/yii2-widgets": "~1.5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "~8.5.4"
    },
    "repositories": [
        {"type": "gitlab", "url": "https://gitlab.com/dicr/yii2-asset.git"},
        {"type": "gitlab", "url": "https://gitlab.com/dicr/yii2-cache.git"},
        {"type": "gitlab", "url": "https://gitlab.com/dicr/yii2-file.git"},
        {"type": "gitlab", "url": "https://gitlab.com/dicr/yii2-widgets.git"},
        {"type": "composer", "url": "https://asset-packagist.org"}
    ]
}
